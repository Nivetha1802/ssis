        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            try
            {
                // Retrieve values from SSIS variables
                string smtpServer = Dts.Variables["User::SmtpServer"].Value.ToString();
                int smtpPort = (int)Dts.Variables["User::SmtpPort"].Value;
                string smtpUsername = Dts.Variables["User::SmtpUsername"].Value.ToString();
                string smtpPassword = Dts.Variables["User::SmtpPassword"].Value.ToString();
                string fromEmail = Dts.Variables["User::FromEmail"].Value.ToString();
                string toEmail = Dts.Variables["User::ToEmail"].Value.ToString();
                string emailSubject = Dts.Variables["User::EmailSubject"].Value.ToString();
                string emailBody = Dts.Variables["User::EmailBody"].Value.ToString();

                // Set up the SMTP client
                ServicePointManager.SecurityProtocol = SecurityProtocolType.Tls12;

                var smtpClient = new SmtpClient(smtpServer)
                {
                    Port = smtpPort,
                    Credentials = new NetworkCredential(smtpUsername, smtpPassword),
                    EnableSsl = true
                };

                // Create the email message
                var mailMessage = new MailMessage
                {
                    From = new MailAddress(fromEmail),
                    Subject = emailSubject,
                    Body = emailBody,
                    IsBodyHtml = true,
                };

                // Add recipient
                mailMessage.To.Add(toEmail);

                // Send the email
                smtpClient.Send(mailMessage);

                // Set the task result to success
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (SmtpException ex)
            {
                Dts.Events.FireError(0, "SMTP Error", ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "General Error", ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }



